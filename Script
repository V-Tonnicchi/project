function startFunction()
{
    document.getElementById('song').play();
}


function timeFunction()
{
    const now = new Date();
    const mill = now.getMilliseconds();

    document.getElementById('demo').innerHTML = mill;
}

const aud = document.getElementById('song');
const now = new Date();
const startmill = now.getMilliseconds();

function extTimeFunction()
{
    if(!aud.paused)
    {
    const press = new Date();
    const pressmill = Math.abs(press.getMilliseconds() - startmill);

    document.getElementById('demo2').innerHTML = pressmill;
    }

}